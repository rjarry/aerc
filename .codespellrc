# ex: ft=dosini

[codespell]
quiet-level = 35
skip =
	*.1,
	*.5,
	*.7,
	*log*,
	*.log*,
	.changelog.md,
	.env,
	contrib/aerc.desktop,
	filters/vectors/*,
	lib/jwz/testdata/*,
	tags,
ignore-words-list =
	DeVault,
	Fo,
	THRID,
	fo,
	fpr,
	froms,
	localY,
	ment,
	struc,
	te,
	thrid,
uri-ignore-words-list = *
ignore-regex = ^(Cc|(Suggested|Requested|Reported|Co-authored|Signed-off|Tested|Reviewed|Acked)-by):\s.+$
